.MaxContainer{
  width: 100%;
}
.MainContent{
  width: 750rpx;
  display: flex;
  flex-wrap: wrap;
  margin-top: -20rpx;
  padding-bottom: 10rpx;
  box-sizing: border-box;
}
.bb{
  width: 100rpx;
  height: 48rpx;
  border-radius: 6rpx;
  background-color: rgb(240, 240, 240);
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left: 20rpx;
  margin-top: 20rpx;
  box-sizing: border-box;
}
.bb text{
  font-size: 24rpx;
  line-height: 48rpx;
  color: rgb(51, 51, 51);
}
.ys{
  background: rgb(53, 152, 165);
}
.ystwo{
  color: #fff !important;
}
