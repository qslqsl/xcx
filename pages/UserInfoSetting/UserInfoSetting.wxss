page{
  background-color: rgb(240, 240, 240);
}
.ContentBox {
	width: 750rpx;
	margin-top: 10rpx;
	background-color: #ffffff;
}
.ContentBoxLine {
	width: 730rpx;
	height: 95rpx;
	margin-left: 20rpx;
	box-sizing: border-box;
	border-bottom: 1rpx solid #ebebeb;
	position: relative;
	display: -webkit-flex;
	display: flex;
	align-items: center;
  justify-content: center;
}
#shaj1,#shaj2 {
	border-bottom: none;
}
.picker{
  font-size: 24rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
	position: absolute;
	right: 70rpx;
	text-align: center;
}
.text {
	width: 54rpx;
	height: 26rpx;
	font-size: 28rpx;
	font-family: PingFang SC;
	font-weight: 400;
	color: rgba(51, 51, 51, 1);
	line-height: 24rpx;
	text-align: center;
	position: absolute;
	left: 0;
	white-space: nowrap;
}
.HeadingImage {
	width: 81rpx;
	height: 77rpx;
  border-radius: 50%;
	position: absolute;
	right: 30rpx;
}
.img_more {
	width: 15rpx;
	height: 28rpx;
	position: absolute;
	right: 30rpx;
	top: 50%;
	margin-top: -14rpx;
}
.text2 {
	font-size: 24rpx;
	font-weight: 400;
	color: rgba(153, 153, 153, 1);
	position: absolute;
	right: 70rpx;
	text-align: center;
}
.AddressSettings {
	width: 750rpx;
	margin-top: 20rpx;
	background-color: #ffffff;
}
