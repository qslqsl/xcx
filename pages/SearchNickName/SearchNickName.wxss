.SearchContainer{
  width: 100%;
}
.SearchContent{
  width: 710rpx;
  height:70rpx;
	position: relative;
  display: flex;
  margin: auto;
}
.Scontent{
  border-radius:6rpx;
  background:rgba(240,240,240,1);
}
.searchImg{
	width: 34rpx;
	height: 34rpx;
  margin: 0 10rpx 18rpx 10rpx;
}
.SearchName{
  display: inline-block;
  width: 576rpx;
  height:70rpx;
	color: #999999;
	font-size:24rpx;
}
.cancel{
  display: inline-block;
  width: 80rpx;
  height: 70rpx;
  line-height: 70rpx;
  text-align: center;
}

.HistoryRecord{
  margin-top: 80rpx;
  margin-left: 20rpx;
}
.HistoryRecord text{
  font-size: 34rpx;
  font-weight: 550;
}

.SearchDiscovery{
  margin-top: 200rpx;
  margin-left: 20rpx;
}

.SearchDiscovery text{
  font-size: 34rpx;
  font-weight: 550;
}