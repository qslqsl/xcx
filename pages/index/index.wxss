page{
  background-color: rgb(240, 240, 240);
  margin-bottom: 102rpx;
}
.IndexTopBar{
  height: 100%;
  display: flex;
  background: #fff;
}
.IndexTopBarCon{
  flex: 1;
  display: flex;
  align-items: center;
}
.IndexTopBarCon view:nth-of-type(1){
  margin-left: 20rpx;
  font-size: 26rpx;
  color: rgba(53,152,165,0.6);
  height: 100%;
  display: flex;
  align-items: center;
}
.IndexTopBarCon view:nth-of-type(2){
  margin: auto;
  margin-right: 45.5%;
  color: rgb(51,51,51);
}


.TitleIndex{
		width:750rpx;
		height:64rpx;
		background:rgba(255,255,255,1);
		display: flex;
		flex-flow: row;
		justify-content: space-between;
		box-sizing: border-box;
		padding: 0 24rpx;
		font-size:28rpx;
		font-weight:400;
		color:rgba(51,51,51,1);
    margin-top: 10rpx;
}
.TitleIndex text{
		line-height: 64rpx;
		height: 100%;
}

.SearchBoxIndex{
		position: relative;
		width: 100%;
		height:70rpx;
		margin-top: 10rpx;
		display: flex;
		align-items: center;
	}
.SearchBoxIndex .SearchInput{
		margin:0 39rpx 0 20rpx;
		width:580rpx;
		height:70rpx;
		background:rgba(255,255,255,1);
		border-radius:6rpx;
		float: left;
		display: flex;
		align-items: center;
}
.SearchBoxIndex .imgSearch{
    width: 34rpx;
	  height: 34rpx;
	  margin-left:13rpx;
}
.SearchBoxIndex .searchClass{
    height: 80%;
	  width:505rpx;
	  margin-left:10rpx;
	  color: #999999;
	  font-size:24rpx;
}
.SearchBoxIndex .ScreenButton{
  	width:90rpx;
		height:54rpx;
		background:rgba(53,152,165,0.6);
		border-radius:27rpx;
		float: right;
		font-size:28rpx;
		font-weight:400;
		color:rgba(255,255,255,1);
		display: flex;
		justify-content: center;
		align-items: center;
		margin-right: 20rpx;
}



