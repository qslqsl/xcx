page{
  background-color: rgb(240, 240, 240);
}
.bg-color{
  background: rgb(53, 152, 165);
  color: #ffffff;
}
.no-ba-color{
  background: #ffffff;
  color: rgb(153, 153, 153);
  border-radius: 10rpx;
}
.show{
  display: block;
}

.content{
		width: 100%;
	}
.abcd{
		width: 730rpx;
		height: 54rpx;
		display: flex;
		align-items: center;
		margin-left: 20rpx;
	}
.text{
		font-size:24rpx;
		font-weight:400;
		color:rgba(153,153,153,1);
	}
.abcde{
		width: 100%;
		min-height:319rpx;
		background:rgba(255,255,255,1);
	}
.abcde1{
		width: 730rpx;
		height: 161rpx;
		margin-left:20rpx;
		box-sizing: border-box;
		border-bottom: 1rpx solid #EBEBEB;
		position: relative;
		display: flex;
		flex-flow: column;
	}
.abcde1 view:first-child{
			align-items: center;
			justify-content: space-between;
		}
.abcde1 view{
			display: flex;
			flex: 1;
		}
.switch_imgbox_openning{
			position: absolute;
			width: 80rpx;
			height: 57rpx;
			right:12rpx;
		}
.switch_imgbox_closed{
			position: absolute;
			width: 80rpx;
			height: 57rpx;
			right:12rpx;
		}
.text2{
		font-size:28rpx;
		font-weight:500;
		color:rgba(51,51,51,1);
	}
.abcde2{
		width: 730rpx;
		height: 157rpx;
		margin-left:20rpx;
		position: relative;
		display: flex;
		flex-flow: column;
	}
.abcde2 view{
			display: flex;
			flex: 1;
		}
.abcde2 view:first-child{
			align-items: center;
			justify-content: space-between;
		}


.con-one{
  background-color: #ffffff;
  margin-top: 10rpx;
}
.one-top{
  display: flex;
  justify-content: space-between;
}
.one-text text{
  display: block;
  font-size: 24rpx;
  width: 336rpx;
}
.one-text text:nth-of-type(1){
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.one-text text:nth-of-type(2){
  margin-left: 20rpx;
}
.one-Btn{
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  border:1px solid rgb(53, 152, 165);
  display: flex;
}
.one-Btn text{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 24rpx;
}
.one-Btn text:nth-of-type(1){
  width: 50%;
  height: 100%;
  border-radius: 8rpx 0 0 8rpx;
}
.one-Btn text:nth-of-type(2){
  width: 50%;
  height: 100%;
  border-radius: 0 8rpx 8rpx 0;
}




