.FocusBox{
  width: 130rpx;
  height: 60rpx;
	border: 1px solid rgba(53, 152, 165, 1);
	border-radius: 35rpx;
  display: flex;
  align-items: center;
  margin-right: 20rpx;
  margin-top: 49rpx;
}
.FocusBox text{
	font-size: 28rpx;
	color: rgba(53, 152, 165, 1);
	margin: auto;
}

/* 遮罩层 */
.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.2;
}
/* 弹出层 */
.modalDlg{
    width: 500rpx;
    position: fixed;
    top: 40%;
    left: 0;
    right: 0;
    z-index: 9999;
    margin: 0 auto;
    background-color: #fff;
    display: flex;
    flex-direction: column;
    align-items: center;
    border-radius: 10rpx;
}
.tcmax-box{
  width: 500rpx;
  height: 250rpx;
}
.box-top{
  width: 100%;
