page{
  background-color: rgb(240, 240, 240);
}

.SearchContainer{
  width: 100%;
  height: 110rpx;
  margin-top: 10rpx;
  background-color: #ffffff;
  display: flex;
  align-items: center;
}
.SearchContent{
  
  width: 710rpx;
  height:70rpx;
  margin: auto;
}
.Scontent{
  display: flex;
  align-items: center;
  border-radius:6rpx;
  background:rgba(240,240,240,1);
}
.searchImg{
  width: 34rpx;
	height: 34rpx;
  margin-left: 10rpx;
}
.SearchName{
  display: inline-block;
  width: 710rpx;
  height:70rpx;
	color: #999999;
	font-size:24rpx;
  margin-left: 10rpx;
}



.mychoose{
  margin-top: 10rpx;
  width: 100%;
  background-color: #ffffff;
}
.mybox{
  width: 710rpx;
  margin: auto;
  padding-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.mybox view{
  font-size: 28rpx;
  color: rgb(153, 153, 153);
}

.mybox-two{
  display: inline;
  background-color: #ffffff;
  overflow: hidden;
}
.Max{
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start; 
  margin: auto;
  margin-bottom: 20rpx;
  padding-bottom: 20rpx;
  width: 710rpx;
}
.mybox-two-con{
  display: flex;
  flex-wrap: wrap;
  padding-bottom: 20rpx;
  
}
.bbbb{
  width: 165rpx;
  height: 80rpx;
  border: 1rpx solid rgb(53, 152, 165);
  border-radius: 6rpx;
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  margin-left: 10rpx;
  margin-top: 20rpx;
}
.bbbb text{
  font-size: 28rpx;
  color: rgb(53, 152, 165);
}

.on{
  color: rgb(53, 152, 165) !important;
}


.ChangWai{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 10rpx;
  background-color: #ffffff;
}
.CWtabbar{
  width: 710rpx;
  height: 60rpx;
  margin: auto;
}
.CWtabbar text{
  display: inline-block;
  width: 80rpx;
  height: 60rpx;
  font-size: 28rpx;
}
.CWtabbar text:nth-of-type(2){
  margin-left: 40rpx;
}

/* 外汇样式 */
.CW-ConBox{
  width: 100%;
  height: 100vh;
  background-color: #ffffff;
}
.MaxTwo{
  width: 710rpx;
  margin: auto;
}
.CW-Con{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
}
.CW-Con view{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 200rpx;
  height: 80rpx;
  margin-top: 20rpx;
  border-radius: 6rpx;
  border: 1rpx solid rgb(188, 188, 188);
}
.CW-Con view{
  font-size: 28rpx;
}
.CW-Con view text:nth-of-type(1){
  color: rgb(51, 51, 51);
}
.CW-Con view text:nth-of-type(2){
  color: rgb(153, 153, 153);
}



